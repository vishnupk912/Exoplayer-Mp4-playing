<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".QuestionPage"
    android:orientation="vertical"
    android:background="@color/white"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/ts48"
        android:background="@color/colorPrimaryDark"
        android:gravity="center_vertical"
        >

        <LinearLayout
            android:id="@+id/backid"
            android:layout_width="@dimen/ts48"
            android:layout_height="@dimen/ts48"
            android:gravity="center"
            android:layout_marginLeft="@dimen/ts12"
            >
            <ImageView
                android:layout_width="@dimen/ts40"
                android:layout_height="@dimen/ts40"
                android:src="@drawable/ic_back"
                />



        </LinearLayout>

        <TextView
            android:id="@+id/propertynameid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:text="Land"
            android:textSize="@dimen/sp16"
            android:layout_marginLeft="@dimen/ts2"
            android:fontFamily="@font/opensanssemibold"
            />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >
        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/viewpager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never"/>

    </LinearLayout>



</LinearLayout>